  "repository": {
    "type": "git",
    "url": "https://github.com/unbywyd/prismalux.git"
  },
  "bugs": {
    "url": "https://github.com/unbywyd/prismalux/issues"
  },
  "homepage": "https://github.com/unbywyd/prismalux#readme",
  "keywords": [
    "prisma",
    "prisma-highlight",
    "prisma-schema",
    "prisma-schema-highlighter",
    "prisma-highlighter",
    "syntax-highlighting",
    "prisma-cli",
    "prisma-tools",
    "developer-tools",
    "cli",
    "typescript",
    "nodejs",
    "terminal"
